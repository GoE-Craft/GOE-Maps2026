{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.tnt_detonator" : {
			"initial_state" : "default",
			"states" : {
				"default" : {
					"transitions": [
                        {"push": "q.cooldown_time_remaining('item_use') > 0"}
                    ]
				},
				"push" : {
					"animations" : [ "push" ],
					"transitions" : [
						{ "default": "q.cooldown_time_remaining('item_use') <= 0 && q.all_animations_finished" }
					],
					"blend_transition" : 0.1
				}
			}
		}
	}
}
