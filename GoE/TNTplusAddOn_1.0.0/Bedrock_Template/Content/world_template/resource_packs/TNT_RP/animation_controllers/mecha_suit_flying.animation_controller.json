{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.goe_tnt.mecha_suit_flying": {
			"initial_state": "not_flying",
			"states": {
				"not_flying": {
					"transitions": [
						{ "flying": "q.property('goe_tnt:is_flying')" }
					]
				},
				"flying": {
					"animations": [ "fly" ],
					"particle_effects": [
						{
							"effect": "mecha_suit_flying",
							"locator": "jetpack_right"
						},
						{
							"effect": "mecha_suit_flying",
							"locator": "jetpack_left"
						}
					],
					"transitions": [
						{ "not_flying": "!q.property('goe_tnt:is_flying')" }
					],
					"blend_transition": 0.1,
					"blend_via_shortest_path": true
				}
			}
		}
	}
}
