{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "goe_tnt:matrix_effect",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/goe/tnt/particles/matrix_effect"
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"creation_expression": "variable.size = 0.05;variable.lifetime = 7.5;"
			},
			"minecraft:emitter_local_space": {
				"position": true,
				"rotation": true
			},
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 100,
				"max_particles": 200
			},
			"minecraft:emitter_lifetime_looping": {
				"active_time": 2
			},
			"minecraft:emitter_shape_sphere": {
				"offset": [0, 1, 0],
				"radius": 5,
				"direction": [0, 25, 0]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "(variable.particle_random_1*variable.lifetime)"
			},
			"minecraft:particle_initial_speed": 5,
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0, 5, 0],
				"linear_drag_coefficient": 10
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["variable.size*(variable.particle_random_1*variable.lifetime-variable.particle_age)/4", "variable.size*(variable.particle_random_1*variable.lifetime-variable.particle_age)*5"],
				"facing_camera_mode": "rotate_y"
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "variable.particle_age/ variable.particle_lifetime",
					"gradient": {
						"0.0": "#FF009EFF",
						"0.1": "#FF00BFFF",
						"0.25": "#FF51FFFA",
						"0.39": "#FF0DD9FF",
						"0.5": "#FF06CBFF"
					}
				}
			}
		}
	}
}