{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "goe_tnt:silent_circle",
			"basic_render_parameters": {
				"material": "particles_add",
				"texture": "textures/goe/tnt/particles/light_fog"
			}
		},
		"curves": {
			"variable.radius": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0, 0.71]
			},
			"variable.explosion": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0.31, 1]
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"creation_expression": "variable.size = 0.5;"
			},
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 45,
				"max_particles": 120
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 2
			},
			"minecraft:emitter_shape_point": {
				"offset": ["variable.radius*-math.sin(variable.emitter_age*720)*2", "variable.radius*math.cos(variable.emitter_age*720)*2", 0],
				"direction": [0, 1, 0]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "math.random(0.1,1.2)"
			},
			"minecraft:particle_initial_speed": 5,
			"minecraft:particle_appearance_billboard": {
				"size": ["variable.size/(variable.particle_age-1.2)", "variable.size/(variable.particle_age-1.2)"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 128,
					"texture_height": 128,
					"uv": [0, 0],
					"uv_size": [128, 128]
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#00FFF6BA",
						"0.1": "#F0FF8B64",
						"0.36": "#D4A2A2A2",
						"0.66": "#E3FF7C5B",
						"1.0": "#00B4B4B4"
					}
				}
			}
		}
	}
}