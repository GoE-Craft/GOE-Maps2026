{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "goe_tnt:timer_on",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/goe/tnt/particles/timer_toggle_particle"
			}
		},
		"components": {
			"minecraft:emitter_rate_instant": {
				"num_particles": 30
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 1
			},
			"minecraft:emitter_shape_sphere": {
				"radius": 0.5,
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.5
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.random(-20,20)",
				"rotation_rate": "math.random(-100,100)"
			},
			"minecraft:particle_initial_speed": 1,
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": ["math.random(-1,1)", "math.random(-1,1)", "math.random(-1,1)"]
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["math.clamp(0.1 / variable.particle_age, 0, 0.1)", "math.clamp(0.1 / variable.particle_age, 0, 0.1)"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 16,
					"texture_height": 128,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [16, 16],
						"step_UV": [0, 16],
						"frames_per_second": 5,
						"max_frame": 30,
						"loop": true
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#FF0DFF8B",
						"0.98": "#FF22FF00"
					}
				}
			}
		}
	}
}