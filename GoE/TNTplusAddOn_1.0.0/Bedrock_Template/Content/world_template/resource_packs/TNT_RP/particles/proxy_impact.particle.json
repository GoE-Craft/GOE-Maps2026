				"size": ["(1 + v.particle_random_1 * v.size) * v.emitter_age * 5", "(1 + v.particle_random_1 * v.size) * v.emitter_age * 5"],
				"facing_camera_mode": "rotate_xyz"
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#B8FFFFFF",
						"0.05": "#FFB5B5B5",
						"0.09": "#FF6F6F6F",
						"0.21": "#BF4A4A4A",
						"0.5": "#FF7A7A7A"
					}
				}
			}
