{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "goe_tnt:glitch_ring",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/goe/tnt/particles/glitch_ring"
			}
		},
		"curves": {
			"variable.stretch": {
				"type": "catmull_rom",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [1, 1, 1, 1, 1, 1, 0]
			},
			"variable.transparency": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0, 1, 0, 1, 0, 1, 0]
			}
		},
		"components": {
			"minecraft:emitter_local_space": {
				"position": true,
				"rotation": true,
				"velocity": true
			},
			"minecraft:emitter_rate_steady": {
				"spawn_rate": "Math.random(10,20)",
				"max_particles": 500
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.5
			},
			"minecraft:emitter_shape_point": {},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "Math.random(0.25, 0.5)"
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.random(90,270)",
				"rotation_rate": "math.sin(v.particle_lifetime * 480 * 2)*100"
			},
			"minecraft:particle_motion_parametric": {
				"relative_position": [0, "math.abs(math.sin(variable.particle_age*360*1))*math.random(0.1,1)", 0],
				"direction": [0, 1, 0]
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["variable.stretch*8", "variable.stretch*8"],
				"facing_camera_mode": "rotate_xyz"
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age * v.particle_lifetime * variable.transparency",
					"gradient": {
						"0.0": "#FFFF2CD8",
						"0.03": "#FF000000",
						"0.05": "#FF372CFF",
						"0.08": "#FF000000",
						"0.11": "#FFFAA300",
						"0.13": "#FF000000",
						"0.15": "#FF61FF69"
					}
				}
			}
		}
	}
}