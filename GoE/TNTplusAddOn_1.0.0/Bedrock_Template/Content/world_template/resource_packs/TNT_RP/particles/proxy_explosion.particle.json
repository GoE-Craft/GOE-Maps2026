{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "goe_tnt:proxy_explosion",
			"basic_render_parameters": {
				"material": "particles_add",
				"texture": "textures/goe/tnt/particles/proxy_particle"
			}
		},
		"curves": {
			"variable.shrink": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0.8, 0]
			}
		},
		"components": {
			"minecraft:emitter_local_space": {
				"position": true,
				"rotation": true
			},
			"minecraft:emitter_rate_instant": {
				"num_particles": 300
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 1
			},
			"minecraft:emitter_shape_sphere": {
				"radius": 0.1,
				"surface_only": true,
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "math.random(0.25,0.5)"
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.random(-360,360)",
				"rotation_rate": "math.random(-100,100)"
			},
			"minecraft:particle_initial_speed": 50,
			"minecraft:particle_motion_dynamic": {
				"linear_drag_coefficient": 1
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["(v.particle_random_1*8) * v.shrink", "(v.particle_random_1*8) * v.shrink"],
				"facing_camera_mode": "rotate_xyz"
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.05": "#D1FF2727",
						"0.39": "#F0B20000",
						"0.49": "#FFFFAA6B",
						"0.59": "#FF9D0000",
						"0.94": "#00FF1C1C"
					}
				}
			}
		}
	}
}