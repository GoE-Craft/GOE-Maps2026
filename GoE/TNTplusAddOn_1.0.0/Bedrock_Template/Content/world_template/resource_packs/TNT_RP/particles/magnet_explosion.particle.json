{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "goe_tnt:magnet_explosion",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/goe/tnt/particles/explosion_particle"
			}
		},
		"curves": {
			"variable.gradient": {
				"type": "linear",
				"input": "variable.particle_random_1",
				"horizontal_range": 1,
				"nodes": [0, 1, 0]
			},
			"variable.pull": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [-0.9, 15, -10]
			}
		},
		"components": {
			"minecraft:emitter_rate_instant": {
				"num_particles": 50
			},
			"minecraft:emitter_lifetime_looping": {
				"active_time": 1
			},
			"minecraft:emitter_shape_sphere": {
				"radius": 2,
				"surface_only": true,
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "Math.random(0.5,1)"
			},
			"minecraft:particle_initial_speed": "math.random(10,20)",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0, 1, 0],
				"linear_drag_coefficient": 4
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["1 * (v.particle_random_2*1.1)", "1 * (v.particle_random_2*1.1)"],
				"facing_camera_mode": "lookat_xyz",
				"uv": {
					"texture_width": 128,
					"texture_height": 2048,
					"flipbook": {
						"base_UV": ["math.floor(v.particle_random_1)*16", 0],
						"size_UV": [128, 128],
						"step_UV": [0, 128],
						"frames_per_second": 16,
						"max_frame": 16,
						"stretch_to_lifetime": true
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_random_2 * v.emitter_random_1",
					"gradient": {
						"0.0": "#94FF0000",
						"0.42": "#94FF0000",
						"0.46": "#941972CD",
						"1.0": "#911972CD"
					}
				}
			}
		}
	}
}