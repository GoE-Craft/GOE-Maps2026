{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "goe_tnt:thief_fog",
			"basic_render_parameters": {
				"material": "particles_add",
				"texture": "textures/goe/tnt/particles/cold_fog"
			}
		},
		"curves": {
			"variable.transparency": {
				"type": "bezier",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [-1.19, 1, 1, -1.2]
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"creation_expression": "variable.radius = 5;v.rot_speed = 720;"
			},
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 30,
				"max_particles": 1000
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 2
			},
			"minecraft:emitter_shape_point": {
				"offset": ["variable.radius*-math.sin(variable.emitter_age* v.rot_speed)", 1, "variable.radius*math.cos(variable.emitter_age*v.rot_speed)"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 1
			},
			"minecraft:particle_expire_if_not_in_blocks": ["minecraft:air"],
			"minecraft:particle_initial_speed": 2,
			"minecraft:particle_motion_dynamic": {},
			"minecraft:particle_appearance_billboard": {
				"size": ["1 + v.particle_random_1*2", "1 + v.particle_random_1*2 + (v.particle_random_2-0.5) * 0.2"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 32,
					"texture_height": 32,
					"uv": [0, 0],
					"uv_size": [32, 32]
				}
			},
			"minecraft:particle_appearance_lighting": {},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#00F3A5FF",
						"0.16": "#FFDD00FF",
						"0.64": "#FFDD00FF",
						"1.0": "#004E0051"
					}
				}
			}
		}
	}
}