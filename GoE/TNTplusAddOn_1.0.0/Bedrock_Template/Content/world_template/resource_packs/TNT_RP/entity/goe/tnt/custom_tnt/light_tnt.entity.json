{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "goe_tnt:light_tnt",
      "materials": {
        "default": "entity_alphatest"
      },
      "textures": {
        "default": "textures/goe/tnt/entity/light_tnt"
      },
      "geometry": {
        "default": "geometry.light_tnt"
      },
			"scripts": {
        "pre_animation": [
          "v.should_swell = query.property('goe_tnt:swell');"
        ],
        "animate": [
          {"charge": "q.mark_variant == 1"},
          {"explode": "q.mark_variant == 2"}
        ]
			},
      "animations": {
        "charge": "animation.light_tnt.charge",
        "explode": "animation.light_tnt.explode"
      },
      "particle_effects": {
        "light_charge_yellow": "goe_tnt:light_charge_yellow",
        "light_charge": "goe_tnt:light_charge",
        "light_sparkles": "goe_tnt:light_sparkles",
        "light_explosion": "goe_tnt:light_explosion",
        "light_big_explosion": "goe_tnt:light_big_explosion",
        "light_sphere": "goe_tnt:light_sphere",
        "light_leftover_sparks": "goe_tnt:light_leftover_sparks"
      },
      "render_controllers": [ "controller.render.tnt" ]
    }
  }
}