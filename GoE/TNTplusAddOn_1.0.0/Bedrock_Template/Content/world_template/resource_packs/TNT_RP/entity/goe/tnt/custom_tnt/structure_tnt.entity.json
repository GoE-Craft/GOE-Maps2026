{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "goe_tnt:structure_tnt",
      "materials": {
        "default": "entity_alphatest"
      },
      "textures": {
        "default": "textures/goe/tnt/entity/structure_tnt"
      },
      "geometry": {
        "default": "geometry.structure_tnt"
      },
			"scripts": {
        "pre_animation": [
          "v.should_swell = query.property('goe_tnt:swell');"
        ],
        "animate": [
          //{"tnt_swelling": "v.should_swell"}, --> Charge and Swell are combined
          {"charge": "q.mark_variant == 1"},
          {"explode": "q.mark_variant == 2"}
        ]
			},
      "animations": {
        //"tnt_swelling": "animation.tnt.swell",
        "charge": "animation.structure_tnt.swell",
        "explode": "animation.structure_tnt.explode"
      },
      "particle_effects": {
        "tnt_charge_yellow": "goe_tnt:tnt_charge_yellow",
        "one_big_puff": "goe_tnt:one_big_puff"
      },
      "render_controllers": [ "controller.render.tnt" ]
    }
  }
}