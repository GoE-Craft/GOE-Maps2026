{
    "format_version": "1.21.130",
    "minecraft:entity": {
        "description": {
            "identifier": "goe_tnt:matrix_tnt_child",
            "is_spawnable": false,
            "is_summonable": true
        },
        "component_groups": {
            "goe_tnt:spawn": {
                "minecraft:environment_sensor": {
                    "triggers": [
                        {
                            "event": "goe_tnt:spawned",
                            "target": "self",
                            "filters": {
                                "test": "on_ground"
                            }
                        }
                    ]
                }
            },
            "goe_tnt:active": {
                "minecraft:mark_variant": {
                    "value": 1
                },
                "minecraft:timer": {
                    "time": 1,
                    "looping": true,
                    "time_down_event": {
                        "event": "goe_tnt:activate",
                        "target": "self"
                    }
                }
            },
            "goe_tnt:wander": {
                "minecraft:mark_variant": {
                    "value": 2
                },
                "minecraft:behavior.move_to_random_block": {
                    "block_distance": 8,
                    "within_radius": 10,
                    "priority": 1
                },
                "minecraft:explode": {
                    "breaks_blocks": true,
                    "causes_fire": false,
                    "destroy_affected_by_griefing": true,
                    "fire_affected_by_griefing": true,
                    "fuse_lit": true,
                    "max_resistance": 0,
                    "fuse_length": [
                        1,
                        3
                    ],
                    "power": 2
                }
            }
        },
        "components": {
            "minecraft:type_family": {
                "family": [ "custom_tnt", "inanimate" ]
            },
            "minecraft:damage_sensor": {
                "triggers": [
                    {
                        "cause": "all",
                        "deals_damage": "no"
                    }
                ]
            },
            "minecraft:body_rotation_blocked": {},
            "minecraft:health": {
                "value": 1,
                "max": 1
            },
            "minecraft:renders_when_invisible": {},
            "minecraft:physics": {
                "has_collision": true,
                "has_gravity": true
            },
            "minecraft:pushable": { "is_pushable": false, "is_pushable_by_piston": false },  
            "minecraft:collision_box": {
                "height": 0.98,
                "width": 0.98
            },
            "minecraft:projectile": {
                "gravity": 0.001,
                "inertia": 0.999
            },
            "minecraft:conditional_bandwidth_optimization": {
                "default_values": {
                    "max_optimized_distance": 80.0,
                    "max_dropped_ticks": 5,
                    "use_motion_prediction_hints": true
                }
            }
        },
        "events": {
            "minecraft:entity_spawned":{
                "sequence": [
                    {
                        "filters": {
                            "test": "on_ground"
                        },
                        "add": {
                            "component_groups": [
                                "goe_tnt:active"
                            ]
                        }
                    },
                    {
                        "filters": {
                            "test": "on_ground",
                            "value": false
                        },
                        "add": {
                            "component_groups": [
                                "goe_tnt:spawn"
                            ]
                        }
                    }
                ]
            },
            "goe_tnt:spawned": {
                "add": {
                    "component_groups": [
                        "goe_tnt:active"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "goe_tnt:spawn"
                    ]
                }
            },
            "goe_tnt:activate": {
                "add": {
                    "component_groups": [
                        "goe_tnt:wander"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "goe_tnt:active"
                    ]
                }
            }
        }
    }
}